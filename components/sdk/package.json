{
  "name": "@ts-cellery/sdk",
  "version": "0.1.0",
  "description": "Cellery for TypeScript",
  "main": "./dist/lang/index.js",
  "types": "./dist/lang/index.d.ts",
  "license": "Apache-2.0",
  "bin": {
    "cellery-scripts": "./dist/scripts/index.js"
  },
  "scripts": {
    "clean": "rimraf ./dist/",
    "format:check": "prettier --check ./src/**/*.ts",
    "format:fix": "prettier --write ./src/**/*.ts",
    "lint": "tslint --fix -c tslint.json -p ./",
    "build": "tsc",
    "build:prod": "npm run clean && npm run format:fix && npm run lint && npm run build"
  },
  "devDependencies": {
    "@types/node": "^10.12.20",
    "commander": "^2.19.0",
    "prettier": "^1.16.2",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4"
  },
  "dependencies": {
    "@types/handlebars": "^4.1.0",
    "@types/js-yaml": "^3.12.0",
    "chalk": "^2.4.2",
    "handlebars": "^4.1.0",
    "js-yaml": "^3.12.2",
    "log": "^3.2.0",
    "log-node": "^3.2.1",
    "rimraf": "^2.6.3"
  }
}
