{
  "name": "@ts-cellery/sdk",
  "version": "0.1.0",
  "description": "Cellery for TypeScript",
  "main": "./dist/lang/index.js",
  "types": "./dist/lang/index.d.ts",
  "license": "Apache-2.0",
  "bin": {
    "cellery-scripts": "./dist/scripts/index.js"
  },
  "scripts": {
    "clean": "rimraf ./dist/ && rimraf ts-cellery-sdk-*.tgz",
    "format:check": "prettier --check ./src/**/*.ts",
    "format:fix": "prettier --write ./src/**/*.ts",
    "build": "tsc && npm pack",
    "build:prod": "npm run clean && npm run format:check && npm run build"
  },
  "devDependencies": {
    "@types/handlebars": "^4.1.0",
    "@types/js-yaml": "^3.12.1",
    "@types/node": "^12.7.12",
    "@types/webpack": "^4.39.2",
    "prettier": "^1.18.2",
    "tslint": "^5.20.0"
  },
  "dependencies": {
    "@babel/core": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "babel-loader": "^8.0.6",
    "commander": "^3.0.2",
    "handlebars": "^4.4.3",
    "js-yaml": "^3.13.1",
    "log": "^6.0.0",
    "log-node": "^7.0.0",
    "rimraf": "^3.0.0",
    "ts-loader": "^6.2.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.6.3",
    "webpack": "^4.41.0"
  }
}
